/var/log/cloudify/rabbitmq/*.log {
        hourly
        missingok
        rotate 7
        size 100M
        compress
        delaycompress
        notifempty
        sharedscripts
        postrotate
            /usr/sbin/rabbitmqctl -n cloudify-manager@localhost rotate_logs
        endscript
        dateext dateformat -%Y%m%d-%s
}