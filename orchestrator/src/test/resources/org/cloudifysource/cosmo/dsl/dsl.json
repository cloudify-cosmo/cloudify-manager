{
    "definitions": {

        "types": {
            "machine": {
                "type": "node",
                "interfaces": {
                    "provisioner": {
                        "provider": "default_provisioner_provider",
                        "operations": [ "create", "start", "stop", "terminate" ]
                    }
                }
            },

            "database_server": {
                "type": "node",
                "properties": {
                    "port": ""
                },
                "interfaces": {
                    "configurer": {
                        "operations": [ "install", "start", "stop", "uninstall" ]
                    }
                }
            },

            "database": {
                "type": "node",
                "properties": {
                    "name": "",
                    "schema_file": ""
                },
                "interfaces": {
                    "configurer": {
                        "operations": [ "create", "delete" ]
                    }
                }
            }

        },

        "relationships": {
            "deployed_on": { },
            "hosted_on": { }
        },

        "providers": {
            "default_provisioner_provider": {
                "url": "plugins/default_provisioner_plugin.zip"
            },
            "default_chef_provider": {
                "url": "plugins/default_provisioner_plugin.zip"
            }
        },

        "service_template": {

            "mysql_machine": {
                "type": "machine",
                "properties": {
                    "image": "aws149180"
                }
            },

            "mysql_database_server": {
                "type": "database_server",
                "properties": {
                    "port": "3306",
                    "chef_recipe": "mysql_chef_recipe"
                },
                "interfaces": {
                    "configurer": {
                        "provider": "default_chef_provider"
                    }
                },
                "relationships": {
                    "hosted_on": "mysql_machine"
                }
            },

            "mysql_schema": {
                "type": "database",
                "properties": {
                    "name": "petclinic"
                },
                "relationships": {
                    "deployed_on": "mysql_database_server"
                }
            }
        }
    }

}